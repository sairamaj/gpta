@{
    ViewData["Title"] = "Home Page";
}

@model IEnumerable<System.Security.Claims.Claim>

<dv>
<table border="1" cellpadding="10">
                    <th>Type</th>
                    <th>Value</th>
                                    
            @foreach(var claim in Model.OrderBy(c=> c.Type)){
                 <tr>
                    <td>@claim.Type </td>
                    <td>@claim.Value </td>
                </tr>
            }
</table>
</dv>